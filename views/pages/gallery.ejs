<title>Tehama Awards | <%= title %> </title>
<!-- Inner Page Header serction start here -->
<div class="inner-page-header">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="header-page-title text-center">
                    <h2>Gallery</h2>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Inner Page Header serction end here -->

<!-- Home Page Gellary Start Here -->
<div class="portfolio-one-area portfolio-two pt-90 pb-100">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="gridFilter portfolio-menu text-center">
                    <ul>
                        <li class="active" data-filter="*">All</li>
                        <li data-filter=".theme">Arusha 2025</li>
                        <li data-filter=".plugin">Dar Es Salaam 2024</li>
                        <li data-filter=".template">Dodoma 2023</li>
                        <li data-filter=".html">Mwanza 2022</li>
                        <li data-filter=".graphics">Mbeya 2021</li>
                    </ul>
                </div><!-- .gridFilter end-->
                <div class="row grid">
                    <% for (let i = 1; i < 132; i++) { %>
                        <!-- gallery item start -->
                        <div class="col-lg-3 col-md-4 col-sm-12 grid-item theme">
                            <div class="single-portfolio mb-4 rounded-3">
                                <div class="portfolio-image">                                
                                    <img src="images/gallery/<%=i%>.jpg" alt="Image">
                                </div>
                                <div class="overley">
                                    <div class="portfolio-details">
                                        <h3> 
                                            <a>View Gallery</a>
                                        </h3>
                                        <ul>
                                            <li>
                                                <a href="images/gallery/<%=i%>.jpg" data-lightbox="example-set" data-title="Tehama Awards">
                                                    <i class="fa fa-search-plus"></i>
                                                </a>
                                            </li>        
                                        </ul>                                            
                                    </div>
                                </div>
                            </div>
                        </div>
                    <% } %>
                </div>
            </div>
        </div>
        <!-- Portfolio One End Here -->
    </div>
</div>
<!-- Home Page Gellary End Here -->